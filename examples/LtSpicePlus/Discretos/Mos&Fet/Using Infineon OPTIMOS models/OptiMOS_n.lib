.PARAM  aubr=61m     lnBr=-23    Ubr=63.4     ka=14f
.PARAM  UTnbr=207m   Wcm=9.07u   ph0=18.37    ph1=30m      dvgs=0.3

.PARAM  Rd=60m       nmu=2.7     Tref=298     T0=273       lnIsj=-28.6
.PARAM  ndi=1        Rdi=11m     nmu2=0.5     ta=40n       td=10n
