.model ENZ SW(Ron={Ro} Roff=1u Vt=500m Vh=-100m Noiseless)
.param Avo=120
.param fp1=148.5 fp2=42k
.param Avo2_dB={Avo-40+1}
.param Avo2 = {pwr(10, (Avo2_dB)/20)}
.param Cfp1={1 / (2 * pi * fp1 * 1Meg)}
.param Cfp2={1 / (2 * pi * fp2 * 1Meg)}
.param Ro=10k
.param R_EG=333
.param GRV=4k
