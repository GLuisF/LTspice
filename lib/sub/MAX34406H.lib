.param a=5.26u b=-216n
.param c=-170p d=9.5p
.param Vcm_min=2 Vcm_max=28
.param Vs_min=2.7 Vs_max=5.5
.param Iscp=10m Iscn={Iscp}
.param Iq_on=200u
