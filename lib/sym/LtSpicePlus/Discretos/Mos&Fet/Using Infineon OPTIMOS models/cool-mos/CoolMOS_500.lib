.SUBCKT cool_500_n_var dd g s Tj t1 PARAMS: a=1 dVth=0 dR=0 Inn=1 Unn=1 Rmax=1 Rs=0 dC=0 heat=1
     
.PARAM  enable_diode=1
        
.PARAM  Wcml=1.6     c=1         Vth0=3.54     auth=5.1m      nb=1.2       
.PARAM  aubr=0.5     lnBr=-23    Ubr=557       UTnbr=207m     al=0.5

.PARAM  Rd=3         nmu=3.3     Tref=298    T0=273       lnIsj=-30.6
.PARAM  ndi=1        Rdi=80m     nmu2=0.5    ta=1u        td=200n
.PARAM  Rf=0.23      ph0=22.15   ph1=37m     rpa=160m     kbq=85.8u
      
.PARAM  w0=40     w1=20k     w2=22.7      x1=4.729      x2=-3
.PARAM  w4=150    w5=10      x3=0.873     x4=-19
.PARAM  w8=6      w9=15      x5=26.376    x6=0.72
.PARAM  w13=210   w14=-0.12
.PARAM  w15=-0.12  w16=120   w17=150   w18=-1  w19=80   w20=100     w21=0.7p   w22=3.6p
.PARAM  f=105p     
       
.PARAM  Vmin=2.64     Vmax=4.44    dCmax=0.33
.PARAM  Vth={Vth0+(Vmax-Vth0)*limit(dVth,0,1)-(Vmin-Vth0)*limit(dVth,-1,0)}
.PARAM  p0={Wcml*((T0/Tref)**nb)*a}
.PARAM  p1={Unn-Inn*Rs-Vth0}
.PARAM  Rlim={((SQRT(p1**2-4*c/p0*Inn)-p1)*(1+al*p1/c)/(2*c*Inn)+al/(p0*c)+(Rmax-Rs))/(1+rpa*(Inn/a)**2)}
*.PARAM  Rlim={(Rmax-(Unn-Vth0-SQRT((Unn-Vth0)**2-4*c*Inn/p0))/(2*c*Inn))/(1+rpa*(Inn/a)**2)} 
.PARAM  dRd={Rd/a+if(dVth==0,limit(dR,0,1)*max(Rlim-Rd/a,0),0)} 
.PARAM  bet={Wcml}
.PARAM  dC1={1+dCmax*limit(dC,0,1)} 

.PARAM  Cox={1p*(w17+w20)*a*dC1}
.PARAM  Cgs0={f*a*dC1}
.PARAM  Cox1={(w21*a+w22)*dC1}
.PARAM  dRdi={Rdi/a}

.FUNC I2(p,Uee,z1,pp) {if(Uee>pp,(Uee-c*z1)*z1,p*(pp-p)/c*exp((Uee-pp)/p))}
.FUNC Ig(Uds,T,p,Uee) {bet*(T0/T)**nb*I2(p,Uee,min(Uds,Uee/(2*c)),min(2*p,p+c*Uds))}
.FUNC Iges(Udsk,Uds,Ugs,T) 
 +{a*(sgn(Udsk)*Ig(abs(Udsk),T,1/(ph0-ph1*T),Ugs-Vth+auth*(T-Tref))+exp(min(lnBr+(abs(Uds)-UBr-aubr*(T-Tref))/UTnbr,25)))}

.FUNC Isjt(Tj)           {exp(min(lnIsj+(Tj/Tref-1)*1.12/(ndi*kbq*Tj),9))*(Tj/Tref)**0}
.FUNC Idiode(Usd,Tj,Iss) {exp(min(log(Iss)+Usd/(ndi*kbq*Tj),9))-Iss}
.FUNC Idiod(Usd,Tj)      {a*Idiode(Usd,Tj,Isjt(Tj))}

.FUNC Qa(U)   {w1/(w0*x2)*(max(U-w2,x1)**x2-x1**x2)+min(U,x1+w2)}
.FUNC Qb(U)   {w5/(w4*x4)*(max(U,x3)**x4-x3**x4)+min(U,x3)}
.FUNC Qc(U)   {(exp(w14*U)-1)/w14}
.FUNC Qd(U)   {w9/(w8*x6)*(max(U,x5)**x6-x5**x6)+min(U,x5)}
.FUNC Qe(U)   {1/w15*(w16/w17*(exp(min(w15*U,log(w17/w16)))-1)+max(w15*U-log(w17/w16),0))}
.FUNC Qf(U)   {1/w18*(w19/w20*(exp(min(w18*U,log(w20/w19)))-1)+max(w18*U-log(w20/w19),0))}

E_Edg     d   ox  VALUE {V(d,g)-((w17*Qe(V(d,g))+w20*Qf(V(d,g))))/(w17+w20)}
C_Cdg     ox  g   {Cox}
C_Cdg1    d   g   {Cox1}

E_Eds      d  edep  VALUE {V(d,s)-(limit(I(V_sense3)/a,-100,100)/1p+w0*Qa(V(d,s))+w13*Qc(V(d,s))+w4*Qb(V(d,s)))/(w0+w4+w13)}
C_Cds   edep     s  {a*(w0+w4+w13)*1p}
E_Eds2    d3 edep2  VALUE {V(d3,s)-Qd(V(d3,s))}
C_Cds2 edep2     s  {a*w8*1p}

C_Cgs     g    s  {Cgs0}

G_chan    d    s  VALUE={Iges((SQRT(1+4*al*V(d,s))-1)/(2*al),V(d,s),V(g,s),T0+limit(V(Tj),-200,350))}
E_RMos   d1    d  VALUE={(I(V_I)-I(V_Idio))*(Rf*dRd+(1-Rf)*dRd*((limit(V(T1),-200,999)+T0)/Tref)**nmu)*(1+rpa*(I(V_I)/a)**2)}
V_I      dd   d1  0
G_diode   s   d2  VALUE={Idiod(V(s,d2),T0+limit(V(Tj),-200,499))}
E_Rdio   d1   d3  VALUE={I(V_Idio)*dRdi*((limit(V(Tj),-200,999)+T0)/Tref)**nmu2}
V_Idio   d3   d2  0

L_L001    a    c  {td/(ta+td)}
R_R001    a    b  {1/ta}
V_sense3  c    0  0
E_E001    b    0  VALUE {enable_diode*I(V_Idio)}

R1        g    s  1G
Rd01      d    s  500Meg
Rd02     d3    s  500Meg
Rd03     d1    d  1k
Rd04     d2    d3 1k

G_Ptot_channel     0 Tj VALUE {heat*LIMIT(V(d,s)*I(V_I),0,100k) }
G_Ptot_Epi         0 t1 VALUE {heat*LIMIT(V(dd,d)*I(V_I),0,100k) }

.ENDS
*$

